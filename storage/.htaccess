# Deny all direct access to storage directory
Order Deny,Allow
Deny from all

# Block access to sensitive files
<Files "*.sqlite">
    Order Deny,Allow
    Deny from all
</Files>

<Files "*.db">
    Order Deny,Allow
    Deny from all
</Files>

<Files "*.log">
    Order Deny,Allow
    Deny from all
</Files>
